<!DOCTYPE html>
<html lang="en">
<head>

  <title>TinyApp</title>
  <link rel="stylesheet" type="text/css" href="css/style.css">

</head>
<body>
  <header>
    <% include ./partials/_header.ejs %>
  </header>

  <div id="sidenav">
    <% include ./partials/_sidenav_user.ejs %>
  </div>

  <div id="main">
    <ul>
        <% Object.keys(urls).forEach((key) => { %>
        <li>
          <%= key %> --> <a href = <%= urls[key]["original"] %>> <%= urls[key]["original"] %> </a>

          Unique visits: <%= urls[key]["uniques"] %>

          <a href=<%= `/urls/${key}` %> > Edit </a>

          <form action=<%= `/urls/${key}/delete?_method=DELETE` %> method="POST" style="float:inline">

            <button type="submit"> Delete </button>
          </form>
        </li>
      <% }); %>
    </ul>
  </div>
</body>
</html>